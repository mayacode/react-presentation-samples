{
  "name": "react-presentation-samples",
  "version": "1.0.0",
  "description": "Samples of code used in my presentations",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.+(js|jsx|json)\"",
    "validate": "npm run lint && npm run prettier -- --list-different",
    "precommit": "lint-staged",
    "hooks:build": "webpack --config src/react/hooks/webpack.dev.config.js",
    "hooks:dev": "webpack-dev-server --config src/react/hooks/webpack.dev.config.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayacode/react-presentation-samples.git"
  },
  "author": "Maja Miarecki <maja.miarecki@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
